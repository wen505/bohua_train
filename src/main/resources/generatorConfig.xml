<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<generatorConfiguration>
    <classPathEntry location="E:\\mybatis generator\\mysql-connector-java-5.1.34.jar"/>
    <context id="mysqlTables" targetRuntime="MyBatis3">

       <!-- <property name="mergeable" value="false"></property>-->

        <!--&lt;!&ndash; 去除注释 显示注释false &ndash;&gt;-->
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <!-- jdbc连接信息 -->
        <jdbcConnection driverClass="com.mysql.jdbc.Driver"
                        connectionURL="jdbc:mysql://localhost:3306/bohua_train"
                        userId="root" password="root"/>

        <!-- java类型解析器 可选配置 -->
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false"/>
        </javaTypeResolver>

        <javaModelGenerator targetPackage="com.bh.train.model" targetProject="E:/demo/bohua_train/src/main/java">
            <property name="enableSubPackages" value="true"/>
            <property name="trimStrings" value="true"/>
        </javaModelGenerator>

        <sqlMapGenerator targetPackage="com.bh.train.dao" targetProject="E:/demo/bohua_train/src/main/java">
            <property name="enableSubPackages" value="true" />
        </sqlMapGenerator>

        <javaClientGenerator type="XMLMAPPER"
                             targetPackage="com.bh.train.dao" targetProject="E:/demo/bohua_train/src/main/java">
            <property name="enableSubPackages" value="true"/>
        </javaClientGenerator>

        <!--<table tableName="bh_dictionary_header" domainObjectName="BhDictionaryHeader"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false">
            &lt;!&ndash;<generatedKey column="user_role_id" sqlStatement="mysql" identity="true"/>&ndash;&gt;
        </table>-->
        <table tableName="bh_dictionary_line" domainObjectName="BhDictionaryLine"
               enableCountByExample="false" enableUpdateByExample="false"
               enableDeleteByExample="false" enableSelectByExample="false"
               selectByExampleQueryId="false">
            <generatedKey column="line_id" sqlStatement="mysql" identity="true"/>
        </table>

    </context>
</generatorConfiguration>
